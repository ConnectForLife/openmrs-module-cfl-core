[
  {
    "id": "cfl.personDashboard",
    "label": "CFL module",
    "description": "CFL configuration of person dashboard",
    "extensions": [
      {
        "id": "cfl.personDashboard.outgoing",
        "extensionPointId": "personDashboard.overallActions",
        "type": "link",
        "order": 1,
        "label": "cfl.initcall.button",
        "icon":"icon-phone-sign",
        "url": "/ws/callflows/person/{{person.uuid}}/out/voxeo-mobile/flows/MainFlow.ccxml?actorType=Caregiver"
      },
      {
        "id": "cfl.personDashboard.includeFragments.changeStatus",
        "extensionPointId": "personDashboard.includeFragments",
        "extensionParams": {
          "provider": "messages",
          "fragment": "patientdashboard/changeStatus"
        }
      },
      {
        "id": "cfl.editPersonInfo",
        "extensionPointId": "personDashboard.overallActions",
        "type": "link",
        "label": "cfl.editPersonInfo.label",
        "url": "cfl/editPersonSection.page?personId={{personId}}&sectionId=demographics&appId=cfl.registerCaregiver",
        "icon": "icon-pencil",
        "order": 2,
        "requiredPrivilege": "App: registrationapp.registerPatient"
      },
      {
        "id": "cfl.basicMessages.personDashboardLink",
        "extensionPointId": "personDashboard.overallActions",
        "type": "link",
        "label": "messages.title",
        "icon": "icon-comment",
        "order": 3,
        "url": "owa/messages/index.html#/messages/{{personId}}&patientUuid={{person.uuid}}"
      },
      {
        "id": "cfl.personDashboard.deletePerson",
        "extensionPointId": "personDashboard.overallActions",
        "type": "script",
        "label": "cfl.deletePerson.label",
        "script": "delPerson.showDeletePersonCreationDialog(&#x27;{{person.uuid}}&#x27;)",
        "icon": "icon-remove",
        "order": 4,
        "requiredPrivilege": "Delete People"
      }
    ]
  }
]
